class UnionFind:
    father = []
    def find(x):
        if self.father[x] == x:
            return x
        self.father[x] = self.find(self.father[x])
        return self.father[x]
        
    def union(a, b):
        root_a = self.find(a)
        root_b = self.find(b)
        if not root_a == root_b:
            self.father[root_a] = root_b
